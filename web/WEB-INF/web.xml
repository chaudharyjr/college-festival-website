<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>registercheck</servlet-name>
        <servlet-class>excelniks.registercheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>toprofile</servlet-name>
        <servlet-class>excelniks.toprofile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>excelniks.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>excelniks.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>excelniks.registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletecontest</servlet-name>
        <servlet-class>excelniks.deletecontest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminauth</servlet-name>
        <servlet-class>excelniks.adminauth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admintablepass</servlet-name>
        <servlet-class>excelniks.admintablepass</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registercheck</servlet-name>
        <url-pattern>/registercheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>toprofile</servlet-name>
        <url-pattern>/toprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletecontest</servlet-name>
        <url-pattern>/deletecontest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminauth</servlet-name>
        <url-pattern>/adminauth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admintablepass</servlet-name>
        <url-pattern>/admintablepass</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
